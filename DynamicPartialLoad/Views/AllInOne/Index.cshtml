@using DynamicPartialLoad.Models;
@model  DynamicPartialLoad.Models.MyModelAllInOne
@{
    ViewBag.Title = "Index con jquery";
}
<span>Actualizado: @DateTime.Now.ToString()</span>
@Html.Partial("_menu")
<h2>@Model.Header</h2>
<div id="indexdiv">

    <h2>Pagina de Inicio con parcial</h2>
    <div>
        <span>Contenido Actualizado: @DateTime.Now.ToString()</span>
        <br>
        <p>Estos datos vienen del controlador, a través del modelo:</p>
        <h2>Title: @Model.IndexMainContent.Title</h2>
